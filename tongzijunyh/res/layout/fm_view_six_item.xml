<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/transparent"
    android:orientation="vertical" 
    >
	
    
    <!--第一行 -->	
    <LinearLayout 
        style="@style/fwh"
         
        >
        <RelativeLayout
            android:layout_marginLeft="2dip"
            
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
            <FrameLayout
                android:layout_centerInParent="true"
			    android:layout_width="114dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    
				    android:id="@+id/iv_a_bg"
				    android:layout_height="128dip"
				    android:layout_width="114dip"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_a"
			        android:layout_height="90dip"
				    android:layout_width="90dip"
				    android:background="@color/transparent"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </RelativeLayout>
        
        <RelativeLayout 
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
                 android:layout_centerInParent="true"
			    android:layout_width="114dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView 
				    android:id="@+id/iv_b_bg"
				    android:layout_height="128dip"
				    android:layout_width="114dip"
				    android:background="@drawable/sixmsg"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_b"
			        android:layout_height="90dip"
				    android:layout_width="90dip"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </RelativeLayout>
        <RelativeLayout 
            android:layout_marginRight="2dip"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
            <FrameLayout
                android:layout_centerInParent="true"
			    android:layout_width="114dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView 
				    android:id="@+id/iv_c_bg"
				    android:layout_height="128dip"
				    android:layout_width="114dip"
				    android:background="@drawable/sixmsg1"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_c"
			        android:layout_height="90dip"
				    android:layout_width="90dip"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </RelativeLayout>
        
    </LinearLayout>
    
    
    
    
    <!--第二行 -->	
    
     <LinearLayout 
        style="@style/fwh"
        
        android:layout_marginTop="107dip"
        >
         <LinearLayout
            android:id="@+id/ll_left"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.47"
            >
           <ImageView 
               android:layout_marginTop="2dip"
               android:layout_height="128dip"
               android:layout_width="55dip"
               android:background="@drawable/six_left"
               />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
               android:layout_centerInParent="true"
			    android:layout_width="115dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    android:id="@+id/iv_d_bg" 
				    android:layout_height="128dip"
				    android:layout_width="115dip"
				    android:background="@drawable/sixmsg1"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_d"
			        android:layout_height="90dip"
				    android:layout_width="90dip"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </RelativeLayout>
        
        <RelativeLayout 
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            >
             <FrameLayout
                 android:layout_centerInParent="true"
			    android:layout_width="115dip"
			    android:layout_height="128dip"
			    android:background="@color/transparent"
			    android:orientation="vertical"
			    android:padding="0dip"
			     >
				<ImageView
				    android:id="@+id/iv_e_bg" 
				    android:layout_height="128dip"
				    android:layout_width="115dip"
				    android:background="@drawable/sixmsg1"
				    android:padding="0dip"
				    />
			    
			    <ImageView 
			        android:id="@+id/iv_e"
			        android:layout_height="90dip"
				    android:layout_width="90dip"
				    android:layout_gravity="center"
				    />
			</FrameLayout>
        </RelativeLayout>
        <LinearLayout 
             android:id="@+id/ll_right"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.47"
            >
             <RelativeLayout 
                 android:layout_width="fill_parent"
                 android:layout_height="fill_parent">
                 
             
           <ImageView 
               android:layout_alignParentRight="true"
               android:layout_height="128dip"
               android:layout_width="55dip"
               android:background="@drawable/six_right"
               />
           </RelativeLayout>
        </LinearLayout>
        
    </LinearLayout>
    
   
</FrameLayout>