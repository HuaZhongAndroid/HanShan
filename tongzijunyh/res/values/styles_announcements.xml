<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="layout_list_item_section" parent="@style/fwh">
	    <item name="android:background">@drawable/list_item_section_bg</item>
	    <item name="android:paddingTop">8dip</item>
	    <item name="android:paddingBottom">8dip</item>
	    <item name="android:paddingLeft">16dip</item>
	    <item name="android:paddingRight">16dip</item>
	</style>
	<style name="layout_list_item_section_nobg" parent="@style/fwh">
	    <item name="android:paddingTop">8dip</item>
	    <item name="android:paddingBottom">8dip</item>
	    <item name="android:paddingLeft">16dip</item>
	    <item name="android:paddingRight">16dip</item>
	</style>
	<style name="layout_list_item_section_sl" parent="@style/fwh">
	    <item name="android:background">@drawable/list_item_section_bg_sl</item>
	    <item name="android:paddingTop">8dip</item>
	    <item name="android:paddingBottom">8dip</item>
	    <item name="android:paddingLeft">16dip</item>
	    <item name="android:paddingRight">16dip</item>
	</style>
	<style name="layout_list_item_section_vertical" parent="@style/layout_list_item_section">
	    <item name="android:orientation">vertical</item>
	</style>
	    <!-- 列表文字 -->
    <style name="tv_nav" parent="ffw">
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="tv_dot" parent="ww">
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/icon_dot</item>
    </style>
    <style name="tv_label" parent="ww">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>
    <style name="tv_label_content" parent="ww">
        <item name="android:textColor">#aeaeae</item>
        <item name="android:textSize">15sp</item>
        <item name="android:drawablePadding">10dip</item>
        <item name="android:paddingRight">10dip</item>
        <item name="android:paddingLeft">6dip</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>
    <style name="tv_label_content_fww" parent="tv_label_content">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_weight">1</item>
    </style>
    <style name="txt_gray_border" parent="ww">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">8dip</item>
        <item name="android:layout_marginLeft">6dip</item>
        <item name="android:layout_marginRight">6dip</item>
        <item name="android:background">@drawable/bg_border_c8c8c8</item>
    </style>
    <!-- imgbutton -->
    <style name="img_button" parent="ww">
        <item name="android:padding">10dip</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="form_section_bottom_border" parent="fwh">
        <item name="android:padding">8dip</item>
        <item name="android:layout_marginBottom">10dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_bottom_line_c8c8c8</item>
    </style>
</resources>